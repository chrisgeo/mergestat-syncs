  "panels": [
    {
      "id": 1,
      "title": "Churn (Loc Touched) vs Throughput (Delivery Units)",
      "type": "xychart",
      "datasource": {
        "type": "grafana-clickhouse-datasource",
        "uid": "clickhouse"
      },
      "gridPos": {
        "h": 12,
        "w": 12,
        "x": 0,
        "y": 0
      },
      "targets": [
        {
          "rawQuery": true,
          "rawSql": "SELECT (x_norm - 0.5) AS x_centered, (y_norm - 0.5) AS y_centered, identity_id AS name, team_id FROM stats.ic_landscape_rolling_30d WHERE map_name = 'churn_throughput' AND as_of_day = toDate(toDateTime(intDiv($__to, 1000))) AND match(ifNull(nullIf(team_id, ''), 'unassigned'), '${team_id:regex}') ORDER BY delivery_units_30d DESC",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "showPoints": "auto",
            "pointSize": {
              "fixed": 5
            },
            "lineWidth": 0
          }
        },
        "overrides": []
      },
      "options": {
        "seriesMapping": "auto",
        "xField": "x_centered",
        "yField": "y_centered",
        "seriesField": "name",
        "xMin": -0.5,
        "xMax": 0.5,
        "yMin": -0.5,
        "yMax": 0.5,
        "xAxis": {
          "centeredZero": true,
          "label": "Churn (normalized)"
        },
